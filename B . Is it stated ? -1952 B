#include <iostream>
#include <string>
#include <algorithm>
 
using namespace std;
 
int main() {
    int t;
    cin >> t;
 
    while (t--) {
        string str;
        cin >> str;
 
       
        size_t found = str.find("it", 0);
 
        if (found != string::npos) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
 
    return 0;
}
